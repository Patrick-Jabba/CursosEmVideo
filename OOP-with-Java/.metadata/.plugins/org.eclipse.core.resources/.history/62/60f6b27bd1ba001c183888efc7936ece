package aula09;

public class Pessoa {
	private String nome;
	private int inteiro;
	private String sexo;
	
	
	public void fazerAniversario() {
		this.idade++;
	}


	public Pessoa(String nome, int inteiro, String sexo) {
		this.nome = nome;
		this.inteiro = inteiro;
		this.sexo = sexo;
	}


	public String getNome() {
		return nome;
	}


	public void setNome(String nome) {
		this.nome = nome;
	}


	private int getInteiro() {
		return inteiro;
	}


	public void setInteiro(int inteiro) {
		this.inteiro = inteiro;
	}


	public String getSexo() {
		return sexo;
	}


	public void setSexo(String sexo) {
		this.sexo = sexo;
	}
	
	
	
	
}
